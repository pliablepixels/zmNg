{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "proxy": "node proxy-server.js",
    "dev:all": "npm run proxy & npm run dev",
    "mock:notifications": "node mock-notification-server.js",
    "dev:notifications": "npm run proxy & npm run mock:notifications & npm run dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:notifications": "playwright test tests/notifications.spec.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "android:sync": "npm run build && npx cap sync android",
    "android:run": "npx cap run android",
    "android": "npm run android:sync && npm run android:run",
    "android:logs": "$ANDROID_HOME/platform-tools/adb logcat | grep -E '(Capacitor|Console|chromium)'",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "ios:sync": "npm run build && npx cap sync ios",
    "ios:run": "npx cap run ios --target A0B6DDE7-FAC6-4A71-AF03-73C4E3E1D2BB",
    "ios": "npm run ios:sync && npm run ios:run",
    "ios:open": "npx cap open ios"
  },
  "dependencies": {
    "@aparajita/capacitor-secure-storage": "^7.1.6",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/filesystem": "^7.1.5",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.66.1",
    "react-router-dom": "^7.9.6",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "vis-data": "^8.0.3",
    "vis-timeline": "^8.4.0",
    "ws": "^8.18.3",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-grid-layout": "^1.3.6",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  }
}
